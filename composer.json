{
    "name": "crudadmin/crudadmin",
    "type": "library",
    "description": "Dynamically Laravel CRUD administration with automatic database migrations",
    "keywords": ["migrations", "database", "framework", "crud", "admin", "administration", "automatic", "laravel", "form"],
    "homepage": "https://crudadmin.com",
    "license": "MIT",
    "authors": [
        { "name": "Marek GogoÄ¾", "email": "info@marekgogol.sk" }
    ],
    "require": {
        "php": ">=7.1",
        "crudadmin/framework": "^3.0.51|dev-3.0-dev",
        "crudadmin/resources" : "^3.0.6|dev-3.0-dev",
        "laravel/framework": "^5.4|^6|^7|^8",
        "laravel/ui": "^1.0|^2.0",
        "gettext/gettext": "^4.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7|~7.0|^8.0",
        "orchestra/testbench": "^3.8|^5.0",
        "orchestra/testbench-dusk": "^3.8|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Admin\\": "src"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Admin\\Tests\\": "tests",
            "Admin\\Tests\\App\\": "tests/Stubs/app"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Admin\\Providers\\AppServiceProvider"
            ]
        }
    }
}
