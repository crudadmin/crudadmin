{
    "name": "crudadmin/crudadmin",
    "type": "library",
    "description": "Dynamically Laravel CRUD administration with automatic database migrations",
    "keywords": ["migrations", "database", "framework", "crud", "admin", "administration", "automatic", "laravel", "form"],
    "homepage": "https://crudadmin.com",
    "license": "MIT",
    "authors": [
        { "name": "Marek GogoÄ¾", "email": "info@marekgogol.sk" }
    ],
    "require": {
        "php": ">=7.1",
        "laravel/framework": "^5.4",
        "doctrine/dbal": "2.6.3",
        "intervention/image": "^2.3.0",
        "gettext/gettext": "^4.6",
        "spatie/image-optimizer": "^1.1",
        "pimple/pimple": "~3.0",
        "league/flysystem": "^1.0.45",
        "league/flysystem-cached-adapter": "^1.0.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7|~7.0",
        "orchestra/testbench": "^3.8",
        "orchestra/testbench-dusk": "^3.8"
    },
    "autoload": {
        "psr-4": {
            "Admin\\": "src",
            "CKSource\\CKFinder\\": "src/Plugins/CKFinder/connector"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Admin\\Tests\\": "tests",
            "Admin\\Tests\\App\\": "tests/Stubs/app"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Admin\\Providers\\AppServiceProvider"
            ]
        }
    }
}
